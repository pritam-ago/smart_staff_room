generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Staff {
    id         Int      @id @default(autoincrement())
    name       String
    seatCode   String   @unique
    status     String   @default("out")
    location   String?
    lastSeen   DateTime @default(now())
    faceId     String?  @unique
    department String?
}
